{
  "name": "react-tggl-client",
  "version": "1.6.1",
  "description": "Tggl React wrapper",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "start": "concurrently \"tsc --watch\" \"cpx \\\"src/**/*.css\\\" dist --watch\"",
    "format": "prettier --write src/.",
    "lint": "eslint src/.",
    "test": "jest  --passWithNoTests",
    "prepublishOnly": "npm test && rm -rf lib && npm run build",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "Nicolas Keller",
  "license": "ISC",
  "dependencies": {
    "tggl-client": "^1.13.2"
  },
  "devDependencies": {
    "@jest/types": "^28.0.2",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
