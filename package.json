{
  "name": "react-tggl-client",
  "version": "3.1.2",
  "description": "Tggl React SDK",
  "keywords": [
    "Tggl",
    "React",
    "SDK",
    "Feature flag",
    "Feature toggle",
    "A/B testing",
    "Remote config",
    "Kill switch"
  ],
  "author": "Nicolas Keller",
  "license": "ISC",
  "homepage": "https://tggl.io/developers/sdks/react",
  "repository": "https://github.com/Tggl/react-tggl-client",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write src/.",
    "lint": "eslint src/.",
    "test": "echo \"no tests\"",
    "prepublishOnly": "npm test && npm run lint && rm -rf lib && npm run build",
    "preversion": "npm test && npm run lint",
    "version": "node scripts/syncVersion.mjs && npm update tggl-client --save && npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "tggl-client": "^3.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
