{
  "name": "react-tggl-client",
  "version": "2.1.1",
  "description": "Tggl React wrapper",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write src/.",
    "lint": "eslint src/.",
    "test": "jest  --passWithNoTests",
    "prepublishOnly": "npm test && npm run lint && rm -rf lib && npm run build",
    "preversion": "npm test && npm run lint",
    "version": "node scripts/syncVersion.mjs && npm update tggl-client --save && npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "Nicolas Keller",
  "license": "ISC",
  "dependencies": {
    "tggl-client": "^1.15.4"
  },
  "devDependencies": {
    "@jest/types": "^28.0.2",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
